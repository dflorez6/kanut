<section class="page-wrapper private-wrapper show-wrapper">
  <div class="container-app">

    <h1>
      <% if I18n.locale == :en %>
        Administrator
      <% else %>
        Administrador
      <% end %>
    </h1>

    <p>
      <strong>Avatar:</strong>
      <%#= @user.image %>
    </p>

    <p>
      <strong>Name:</strong>
      <%= @administrator.name %>
    </p>

    <p>
      <strong>Email:</strong>
      <%= @administrator.email %>
    </p>

    <%= link_to 'Edit', edit_administrator_path(@administrator) %> |
    <%= link_to 'Back', administrators_path %>

  </div>
</section>